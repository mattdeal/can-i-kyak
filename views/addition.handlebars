<script>
$(document).ready(function(){
  $(document).on('click', '#submit', function(e) {
    e.preventDefault();

    console.log("test");

    var name = $("#Location");
    var street = $("#Street");
    var city = $("#City");
    var state = $("#State");
    var zip = $("#Zipcode");

    var newPost = {
      name: name.val().trim(),
      street: street.val().trim(),
      city: city.val().trim(),
      state: state.val(),
      zip: zip.val().trim(),
      googleId: "test",
      imageUrl: "test"
    };

    $.post("/api/location", newPost, function(data) {
      console.log("done");
      window.location = ("/location/"+ data.id);
    });
  });//end on.click

});

</script>
